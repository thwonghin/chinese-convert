{
    "name": "@thwonghin/chinese-convert",
    "version": "1.4.2",
    "description": "Command line interface to convert between Chinese text.",
    "repository": "git@github.com:thwonghin/chinese-convert.git",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Hin Wong <thwong.henry@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=14.15.4"
    },
    "scripts": {
        "test:unit": "jest tests/unit",
        "test:integration": "jest tests/integration",
        "clean": "rm -rf dist || true",
        "start": "yarn build && node dist/index.js",
        "build": "tsc",
        "lint": "xo",
        "lint:fix": "xo --fix",
        "version": "npx conventional-changelog-cli -p chinese-convert -i CHANGELOG.md -s && git add CHANGELOG.md",
        "prepublishOnly": "npm shrinkwrap && yarn build"
    },
    "keywords": [
        "cli",
        "translation",
        "chinese",
        "traditional-chinese",
        "simplified-chinese",
        "zhconvert"
    ],
    "bin": {
        "chinese-convert": "./dist/index.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "/dist/**/*.js",
        "/dist/**/*.d.ts",
        "LICENSE",
        "README.md",
        "package.json",
        "npm-shrinkwrap.json"
    ],
    "dependencies": {
        "axios": "^0.21.1",
        "fast-glob": "^3.2.5",
        "iconv-lite": "^0.6.2",
        "jschardet": "^2.2.1",
        "yargs": "^16.2.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.22",
        "@types/yargs": "^16.0.0",
        "axios-mock-adapter": "^1.19.0",
        "eslint-plugin-jest": "^24.1.3",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3",
        "xo": "^0.37.1"
    }
}
